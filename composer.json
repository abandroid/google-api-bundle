{
    "name": "endroid/google-api-bundle",
    "description": "Endroid Google API for Symfony",
    "keywords": ["endroid", "google", "api", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidGoogleApiBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "google/apiclient": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\GoogleApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Bundle\\GoogleApiBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
